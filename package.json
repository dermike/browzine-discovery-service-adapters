{
  "name": "browzine-summon-adapter",
  "version": "0.0.1",
  "description": "Summon adapter to augment journal results with custom BrowZine API data.",
  "engines": {
    "node": "0.10.32"
  },
  "main": "server.js",
  "scripts": {
    "server": "http-server --cors -p 8080",
    "tunnel": "ngrok http 8080",
    "compile": "babel src/ -d dist/ --watch --presets env",
    "compile-ci": "babel src/ -d dist/ --presets env",
    "test": "karma start",
    "deploy-staging": "AWS_BUCKET_FOLDER=summon-staging deploy-web-to-s3 dist",
    "deploy-production": "AWS_BUCKET_FOLDER=summon deploy-web-to-s3 dist"
  },
  "dependencies": {},
  "devDependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-mocks": "1.6.4",
    "angular-resource": "1.6.4",
    "angular-route": "1.6.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "deploy-web-to-s3": "^1.2.1",
    "http-server": "^0.10.0",
    "jquery": "^2.1.3",
    "karma": "1.7.1",
    "jasmine-core": "2.8.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "ngrok": "^2.2.21",
    "recursive-replace": "^1.0.1"
  }
}
